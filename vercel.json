{
    "version": 2,
    "builds": [
       {
         "src": "manage.py",
         "use": "@vercel/python",
         "config": {
           "pythonVersion": "3.11",
           "requirementsTxt": "req.txt"
         }
       }
    ],
    "routes": [
       {
         "src": "/(.*)",
         "dest": "manage.py"
       }
    ]
   }